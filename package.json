{
  "name": "electronuxt",
  "version": "1.0.0",
  "description": "Create an Electron app that serves a Nuxt.js app",
  "main": "main.js",
  "repository": "git@github.com:detrohutt/electronuxt.git",
  "author": "Alexander Roberts <detrohutt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "nuxt build",
    "start": "electron .",
    "pack": "npm run build && electron-packager ./ --overwrite",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "cross-env": "^3.1.4",
    "electron": "^1.4.15",
    "mal-scraper": "^1.2.2",
    "nuxt": "latest",
    "url-parse": "^1.5.0",
    "snyk": "^1.29.0"
  },
  "snyk": true
}
