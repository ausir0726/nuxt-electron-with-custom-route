{
  "name": "electronuxt",
  "version": "1.0.0",
  "description": "Create an Electron app that serves a Nuxt.js app",
  "main": "main.js",
  "repository": "git@github.com:detrohutt/electronuxt.git",
  "author": "Alexander Roberts <detrohutt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "nuxt build",
    "start": "electron .",
    "pack": "npm run build && electron-packager ./ --overwrite",
    "snyk-protect": "snyk-protect",
    "prepublish": "yarn run snyk-protect"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "cross-env": "^3.1.4",
    "electron": "^19.0.15",
    "mal-scraper": "^1.4.0",
    "nuxt": "2.3.0",
    "url-parse": "^1.5.9",
    "@snyk/protect": "latest"
  },
  "snyk": true
}
